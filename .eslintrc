{
  "rules": {
    "indent": [
      2,
      2
    ],
    "quotes": 0,
    "linebreak-style": [
      2,
      "unix"
    ],
    "semi": [
      2,
      "always"
    ],
    "global-strict": [0, "never"],
    "strict": [
      0,
      "global"
    ],
    "no-underscore-dangle": 0,
    "camelcase": 0
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "plugins": [
    "react"
  ]
}
